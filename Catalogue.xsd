<?xml version="1.0" encoding="utf-8"?>
<xs:schema  xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified">


<!-- this is ambiguous -->
<xs:simpleType name="car_brands">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Toyota"/>
		<xs:enumeration value="VW"/>
		<xs:enumeration value="Ford"/>
		<xs:enumeration value="Honda"/>
		<xs:enumeration value="Nissan"/>
		<xs:enumeration value="Mercedes"/>
		<xs:enumeration value="BMW"/>
		<xs:enumeration value="Audi"/>
		<xs:enumeration value="Volvo"/>
		<xs:enumeration value="Mazda"/>
		<xs:enumeration value="Opel"/>
		<xs:enumeration value="Peugeot"/>
		<xs:enumeration value="Renault"/>
		<xs:enumeration value="Dacia"/>
		<xs:enumeration value="Seat"/>
	</xs:restriction>
</xs:simpleType>

<!-- simple types predefinition -->
<xs:simpleType name="price_int">
	<xs:restriction base="xs:integer">
		<xs:minInclusive value="0"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="transmission_type">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Manual"/>
		<xs:enumeration value="Automatic"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="production_year">
	<xs:restriction base="xs:gYear">
		<xs:minInclusive value="1912"/>
		<xs:maxInclusive value="2024"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="bulgarian_regions">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Blagoevgrad"/>
	  	<xs:enumeration value="Burgas"/>
	  	<xs:enumeration value="Dobrich"/>
	  	<xs:enumeration value="Gabrovo"/>
	  	<xs:enumeration value="Haskovo"/>
	  	<xs:enumeration value="Kardzhali"/>
	  	<xs:enumeration value="Kyustendil"/>
	  	<xs:enumeration value="Lovech"/>
	  	<xs:enumeration value="Montana"/>
	  	<xs:enumeration value="Pazardzhik"/>
	  	<xs:enumeration value="Pernik"/>
	  	<xs:enumeration value="Pleven"/>
	  	<xs:enumeration value="Plovdiv"/>
	  	<xs:enumeration value="Razgrad"/>
	  	<xs:enumeration value="Ruse"/>
	  	<xs:enumeration value="Shumen"/>
	  	<xs:enumeration value="Silistra"/>
	  	<xs:enumeration value="Sliven"/>
	  	<xs:enumeration value="Smolyan"/>
	  	<xs:enumeration value="Sofia"/>
	  	<xs:enumeration value="Sofia City"/>
	  	<xs:enumeration value="Stara Zagora"/>
	  	<xs:enumeration value="Targovishte"/>
	  	<xs:enumeration value="Varna"/>
	  	<xs:enumeration value="Veliko Tarnovo"/>
	  	<xs:enumeration value="Vidin"/>
	  	<xs:enumeration value="Vratsa"/>
	  	<xs:enumeration value="Yambol"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="colour_palette">
	<xs:restriction base="xs:string">
		<xs:enumeration value="red"/>
		<xs:enumeration value="blue"/>
		<xs:enumeration value="green"/>
		<xs:enumeration value="yellow"/>
		<xs:enumeration value="orange"/>
		<xs:enumeration value="grey"/>
		<xs:enumeration value="white"/>
		<xs:enumeration value="black"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="engine_types">
	<xs:restriction base="xs:string">
		<xs:enumeration value="Diesel engine"/>
		<xs:enumeration value="Petrol engine"/>
		<xs:enumeration value="Electric engine"/>
		<xs:enumeration value="Diesel-electric engine"/>
		<xs:enumeration value="Petrol-electric engine"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="horsepower_int">
	<xs:restriction base="xs:integer">
		<xs:minInclusive value="50"/>
		<xs:maxInclusive value="2000"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="miliage_int">
	<xs:restriction base="xs:integer">
		<xs:minInclusive value="0"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="ecoStandarts"> 
	<xs:restriction base="xs:string">
		<xs:enumeration value="EURO 1"/>
		<xs:enumeration value="EURO 2"/>
		<xs:enumeration value="EURO 3"/>
		<xs:enumeration value="EURO 4"/>
		<xs:enumeration value="EURO 5"/>
		<xs:enumeration value="EURO 6"/>
		<xs:enumeration value="EURO 7"/>
	</xs:restriction>
</xs:simpleType>

<xs:simpleType name="engine_volume">
	<xs:restriction base="xs:decimal">
		<xs:minInclusive value="1.0"/>
		<xs:maxInclusive value="7.0"/>
	</xs:restriction>
</xs:simpleType>


<!-- root element -->
<xs:element name="catalogue">
	<xs:complexType>
		<xs:element ref="carList"/>
	<xs:complexType>
</xs:element>

<!-- list element -->
<xs:element name="carList">
	<xs:complexType>
		<xs:element ref="car" maxOccurs="unbounded"/>
	</xs:complexType>
</xs:element>

<!-- main element -->
<xs:element name="car">
	<xs:complexType>
		<xs:sequence>
			<xs:element ref="brand"/>
			<xs:element ref="year"/>
			<xs:element ref="engine"/>
			<xs:element ref="transmission"/>
			<xs:element ref="price"/>
			<xs:element ref="location"/>
			<xs:element ref="colour"/>
		</xs:sequence>
	</xs:complexType>
</xs:element>


<!-- car elements -->
