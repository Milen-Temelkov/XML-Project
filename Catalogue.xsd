<xs:element>

<xs:element name="engine">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="volume" type="engine_volume"/>
			<xs:element name="type" type="engine_types"/>
			<xs:element name="power" type="horsepower_int"/>
			<xs:element name="miliage" type="miliage_int"/>
			<xs:element name="ecoStandart" type="ecoStandarts"/>
		<xs:sequence>
	</xs:complexType>
</xs:element>

<xs:element name="year" type="production_year"/>

<xs:element name="transmission" type="transmission_type"/>

<xs:element name="price" type="price_int"/>

<xs:element name="location" type="bulgarian_regions"/>

<xs:element name="colour" type="colour_palette"/>